{
    "version": 2,
    "name": "real-estate-pro",
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/services",
        "dest": "/server.js"
      }
    ]
  }